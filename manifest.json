{
    "manifest_version": 2,
    "name": "SIGAA UnB",
    "version": "1.0",
    "description": "Tenha os códigos de horários traduzidos e visualize suas aulas de forma simples e descomplicada com uma grade horária",
    "icons": {
        "16": "./icons/obj-16x16.png",
        "24": "./icons/obj-24x24.png",
        "32": "./icons/obj-32x32.png",
        "48": "./icons/obj-48x48.png",
        "128": "./icons/obj-128x128.png"
    },
    "permissions": [
        "https://sig.unb.br/sigaa/*",
        "webNavigation",
        "tabs"
    ],
    "background": {
        "scripts": ["./background.js"],
        "persistent": true
    },
    "homepage_url": "https://sig.unb.br/sigaa/portais/discente/discente.jsf"
  }